docker run python:0.0.1 -v /Users/mohammadumaid/WeCloudData/docker/input:/usr/app/src \ 
-v /Users/mohammadumaid/WeCloudData/docker/output:/usr/app/src \ 
-e INPUT_DIR='input' \ 
-e OUTPUT_DIR='output'

docker image build -t python:0.0.1 .

docker image rm


docker rm $(docker ps -a -f status=exited -f status=created -q)
docker image rm python:0.0.1

docker run -t -d python:0.0.1

docker exec -i -t python:0.0.1 /bin/bash